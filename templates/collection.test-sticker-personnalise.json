    "new_rich_text_F8mtLT": {
      "type": "new-rich-text",
      "blocks": {
        "heading_HtyH33": {
          "type": "heading",
          "settings": {
            "heading": "Stickers autocollants personnalisés",
            "heading_type": "h2",
            "heading_size": "h1"
          }
